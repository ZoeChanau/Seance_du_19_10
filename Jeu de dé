from random import random
from random import randint

x=randint(1,6) 
print x

if x == 1 or x==2 or x==6 :
    print "gagné"
else :
    print "perdu"

# Avec liste

from random import random
from random import randint


x=randint(1,6) 
print x

if x in [1,2,6]:
    print "gagné"
else :
    print "perdu"
